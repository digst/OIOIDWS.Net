    </bindings>
    <client>
      <!--HTTP:<endpoint address="https://digst.oioidws.wsp:9090/helloworld" behaviorConfiguration="SoapBehaviourConfiguration" binding="SoapBinding" bindingConfiguration="SoapBindingConfiguration" contract="HelloWorldProxy.IHelloWorld">-->
