<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" xmlns="http://www.w3.org/2001/XMLSchema">
  <include schemaLocation="Modificator.xsd" />
  <include schemaLocation="ReviewedDateTime.xsd" />
  <element name="Reviewed" type="medicinecard20150101:ReviewedType">
    <annotation>
      <documentation xml:lang="en-GB">Information about who Reviewed the medicine card, at admission to hospital, including a timestamp when this occurred</documentation>
      <documentation xml:lang="da-DK">Information hvem der evaluerede medicinkortet, ved indlæggelse på sygehus, inklusiv tidspunktet</documentation>
    </annotation>
  </element>
  <complexType name="ReviewedType">
    <sequence>
      <element name="By" type="medicinecard20150101:ModificatorType" />
      <element name="DateTime" type="medicinecard20150101:ReviewedDateTimeType" />
    </sequence>
  </complexType>
</schema>