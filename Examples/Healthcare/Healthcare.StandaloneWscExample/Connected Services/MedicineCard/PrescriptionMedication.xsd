      <element minOccurs="0" name="ReceiverEAN" type="medicinecard20150101:ReceiverEANType" />
      <element name="Status" type="medicinecard20150101:PrescriptionMedicationStatusType" />
      <element minOccurs="0" name="Deprecated" type="medicinecard20150101:DeprecatedDateTimeType" />
      <element minOccurs="0" maxOccurs="unbounded" name="Effectuation" type="medicinecard20150101:EffectuationOnPrescriptionMedicationType" />
      <element minOccurs="0" maxOccurs="unbounded" name="InvalidEffectuation" type="medicinecard20150101:InvalidEffectuationType" />
    </sequence>
