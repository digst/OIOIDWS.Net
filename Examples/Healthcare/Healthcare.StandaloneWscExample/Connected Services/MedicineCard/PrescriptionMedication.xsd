      <element minOccurs="0" name="TerminatedDateTime" type="medicinecard20150101:TerminatedDateTimeType" />
      <choice minOccurs="0" maxOccurs="3">
        <element name="DeliveryInformation" type="medicinecard20150101:DeliveryInformationType" />
        <element name="OrderInstruction" type="medicinecard20150101:OrderInstructionType" />
      </choice>
      <element minOccurs="0" name="Delivery" type="medicinecard20150101:DeliveryType" />
      <element minOccurs="0" name="ReimbursementClause" type="medicinecard20150101:ReimbursementClauseType" />
