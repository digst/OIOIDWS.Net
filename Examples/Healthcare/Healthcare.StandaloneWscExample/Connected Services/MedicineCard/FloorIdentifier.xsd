<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" xmlns:xkom="http://rep.oio.dk/xkom.dk/xml/schemas/2006/01/06/" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" xmlns="http://www.w3.org/2001/XMLSchema">
  <element name="FloorIdentifier" type="medicinecard20150101:FloorIdentifierType">
    <annotation>
      <documentation>
			Identification which describes the floor or level on which a specific entrance door, appartment or suite is placed, in the staircase refered to.
			</documentation>
    </annotation>
  </element>
  <simpleType name="FloorIdentifierType">
    <restriction base="string">
      <pattern value="[1-9]|[1-9][0-9]|KL|ST|kl|st|k[2-9]|K[2-9]" />
    </restriction>
  </simpleType>
</schema>