<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" xmlns="http://www.w3.org/2001/XMLSchema">
  <element name="DrugMedicationCreatedDateTime" type="medicinecard20150101:DrugMedicationCreatedDateTimeType">
    <annotation>
      <documentation xml:lang="en-GB">The date and time the drug medication is started</documentation>
      <documentation xml:lang="da-DK">Dato og tidspunkt lægemiddelordinationen påbegyndes</documentation>
