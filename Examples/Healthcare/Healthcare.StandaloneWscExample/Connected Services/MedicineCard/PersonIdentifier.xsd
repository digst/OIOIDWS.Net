<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" xmlns="http://www.w3.org/2001/XMLSchema">
  <element name="PersonIdentifier" type="medicinecard20150101:PersonIdentifierType">
    <annotation>
      <documentation xml:lang="en-GB">Element for a person identifier, according to the Danish CPR format.</documentation>
      <documentation xml:lang="da-DK">Element for er CPR-nummer</documentation>
    </annotation>
  </element>
  <simpleType name="PersonIdentifierType">
    <restriction base="string">
      <pattern value="((((0[1-9]|1[0-9]|2[0-9]|3[0-1])(01|03|05|07|08|10|12))|((0[1-9]|1[0-9]|2[0-9]|30)(04|06|09|11))|((0[1-9]|1[0-9]|2[0-9])(02)))[0-9]{6})" />
    </restriction>
  </simpleType>
</schema>