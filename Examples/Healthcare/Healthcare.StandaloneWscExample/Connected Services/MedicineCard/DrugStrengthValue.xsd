<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" xmlns="http://www.w3.org/2001/XMLSchema">
  <element name="DrugStrengthValue" type="medicinecard20150101:DrugStrengthValueType">
    <annotation>
      <documentation xml:lang="en-GB">Drug strength, numerical value (DMA01 field 11)</documentation>
      <documentation xml:lang="da-DK">Lægemiddelstyrke, numerisk værdi (LMS01 felt 11)</documentation>
    </annotation>
  </element>
  <simpleType name="DrugStrengthValueType">
    <restriction base="decimal">
      <fractionDigits value="3" />
      <totalDigits value="10" />
      <minInclusive value="0.000" />
    </restriction>
  </simpleType>
</schema>