<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:medicinecard20150101="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.dkma.dk/medicinecard/xml.schema/2015/01/01" xmlns="http://www.w3.org/2001/XMLSchema">
  <element name="PackageQuantity" type="medicinecard20150101:PackageQuantityType">
    <annotation>
      <documentation xml:lang="en-GB">Contains the numer of packages</documentation>
      <documentation xml:lang="da-DK">Angiver antallet af pakninger</documentation>
    </annotation>
  </element>
  <simpleType name="PackageQuantityType">
    <restriction base="integer">
      <minInclusive value="1" />
      <maxInclusive value="9999" />
    </restriction>
  </simpleType>
</schema>